{
  "expo": {
    "name": "Sportify360",
    "slug": "Sportify360",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./splashicons/sportify-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./splashicons/sportify-logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.Sportify360"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./splashicons/sportify-logo.png"
    },
    "splash": {
      "image": "./splashicons/sportify-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./splashicons/sportify-logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/SpaceMono-Regular.ttf",
            "./assets/fonts/Bangers-Regular.ttf",
            "./assets/fonts/PermanentMarker-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "21ddd377-ead9-4abd-8a64-6776924d9236"
      }
    }
  }
}
