{
  "expo": {
    "name": "Sportify360",
    "slug": "Sportify360",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image":"../assets/images/bg-logo.jpg",
          "resizeMode":"cover",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-font",
        {
          "fonts":[
            "./assets/fonts/SpaceMono-Regular.ttf",
            "./assets/fonts/Bangers-Regular.ttf",
            "./assets/fonts/PermanentMarker-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
